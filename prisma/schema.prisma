datasource db {
    provider = "sqlite"
    url = env("DATABASE_URL")
}
  
  generator client {
    provider = "prisma-client-js"
}
  
model Article {
    slug        String @id
    title       String
    tag         String
    intro       String
    image       String
    markdown    String
    length      Int
  
    createdAt   DateTime @default (now())
}